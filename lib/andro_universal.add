// =================================================================
// Errors and Events, master and various details
// =================================================================
column syslog         { description: Log Number;        type_id: int; }
column syslog_type    { description: Log Entry Type;    type_id: char; colprec: 10; }
column syslog_subtype { description: Log Entry Subtype; type_id: char; colprec: 10; }
column syslog_name    { description: Log Entry Name;    type_id: char; colprec: 25; }
column syslog_text    { description: Log Entry Text;    type_id: text; }
table syslogs {
	description: Processing Logs;
    uisort: 500;
	module: system;

	column syslog { primary_key: Y; uisearch: Y; automation_id: SEQUENCE; }
   column syslog_type { uisearch: Y; }
   column syslog_subtype { uisearch: Y; }
   column syslog_name    { uisearch: Y; }
   column { syslog_text; }
   column { ts_ins; uid_ins; }

   group $LOGIN { permins: Y; permsel: Y; }

   index syslog_subtype { column { syslog_subtype; }}
   index syslog_name    { column { syslog_name;    }}
}

column syslog_enum     { description: Ref Number; type_id: int; automation_id: SEQUENCE; }
//column syslog_etype    { description: Error Type;       type_id: char;  colprec: 10;}
//column syslog_etrxtype { description: Transaction Type; type_id: vchar; colprec: 25;}
column syslog_etext    { description: Error Message;    type_id: text; }
table syslogs_e {
   description: Processing Log Details;
   uisort: 501;
   module: system;

   column syslog_enum     { primary_key: Y; uisearch: Y; }
   foreign_key syslogs    { uisearch: Y; delete_cascade: Y; }
   //column syslog_etype    { uisearch: Y; }
   //column syslog_etrxtype { uisearch: Y; }
   column { syslog_etext; }
   column { ts_ins; uid_ins; }

   group $LOGIN { permins: Y; permsel: Y; }
   //index etrxtype { column { syslog_etrxtype; }}
}

