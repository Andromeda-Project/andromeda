column receiver_id   {description: Receiver ID;   type_id: char; colprec: 15; }
column notify_version{description: Notify version;type_id: char; colprec: 5; }

table pptransactions {
   module: system;
   uisort: 300;
   description: Paypal Trx Log;

   column recnum { suffix: _pp; primary_key: Y; uisearch: Y; }
