// =================================================================
// =================================================================
// USER-DEFINED REPORTS
// -----------------------------------------------------------------
// There are three basic tables: the report, its tables, and
// their columns.  JOINs are worked out automatically by the
// report-writer code.  This table is in its own module
//
// =================================================================
// =================================================================
module reports750 {
   description: Reports;
   uisort: 750;
   nomenu: Y;
}

column report     { description: Report Code; type_id:vchar; colprec: 20; }
column repfilters { description: Filters; type_id: text; }
table reports {
   module: reports750;
   description: Report Writer;
   uisort: 900;

   column report { primary_key: Y; uisearch: Y; }
   column description { uisearch: Y; }
   column repfilters { uino: Y; }
   column flag {
      suffix: _land;
      description: Landscape Mode;
   }
   column { ts_ins; uid_ins; ts_upd; uid_upd; }

}

// -- NOTE.  Table_id is a column, not a foreign key.  This is because
//    we do not yet trust our list of tables.  We also have not worked
//    out how to handle somebody dropping from the dd a table used
//    in reports.  A dropped table will not have a client-side ddtable_
//    file, which should mess up the report writer good.
table reporttables {
   module: reports750;
   description: Report Tables;
   nomenu: Y;

   foreign_key reports { primary_key: Y; uisearch: Y; }
   column table_id     { primary_key: Y; uisearch: Y; }
   column { ts_ins; uid_ins; ts_upd; uid_upd; }

}

// -- NOTE.  Again there neither table_id nor column_id is a foreign
//    key.  Client-side code must enter something that makes sense.
column compval { description: Comparison Value; type_id:vchar; colprec:100;}
table reportcolumns {
   module: reports750;
   description: Report Columns;
   nomenu: Y;

   foreign_key reports { primary_key: Y; uisearch: Y; }
   column table_id     { primary_key: Y; uisearch: Y; }
   column column_id    { primary_key: Y; uisearch: Y; }
   column uicolseq { description: Display Order;
      automation_id: BLANK;
   }
   // A column appears in the sort order if checked.  The sequence
   // of columns in the sort clause goes by display order.
   column flag { suffix: _sort; description: Sort Flag; }
   //column uisort   { description: Sort Sequence; automation_id: BLANK; }
   //column flag { suffix: _summ; description: Summarize On; }
   //foreign_key { summacts;
   //   column summact { automation_id: BLANK; }
   //}
   column { description; }
   //column { compoper; compval; automation_id: BLANK; }
   column { type_id; colprec; colscale; dispsize; }

   column { ts_ins; uid_ins; ts_upd; uid_upd; }

   index uicolseq { column { uicolseq; } }
}

column reportlevel { type_id: int; description: Report Level; }
table reportlevels {
   module: reports750;
   description: Report Levels;
   nomenu: Y;

   foreign_key reports { primary_key: Y; uisearch: Y; }
   column reportlevel  { primary_key: Y; }

   // The user interface always puts levels here.
   column flag { suffix: _skipdetail; description: Skip Detail; uino: Y; }
   column flag { suffix: _count;      description: Count Rows;  uino: Y; }
   column { repfilters; }
}


table reportcollevels {
   module: reports750;
   description: Report Columns by Level;
   nomenu: Y;

   foreign_key reportcolumns  { primary_key: Y; uisearch: Y; }
   foreign_key reportlevels   { auto_insert: Y; uisearch: Y; nocolumns: Y; }
   column reportlevel { primary_key: Y; }
   foreign_key { summacts; }

   column { ts_ins; uid_ins; ts_upd; uid_upd; }

   //index uicolseq { column { uicolseq; } }
}



column summact { description: Summary As; type_id:char; colprec: 5; }
table summacts {
   module: reports750;
   description: Summary Actions;
   nomenu: Y;

   column summact { primary_key: Y; uisearch: Y; }
}

content summacts {
   columns { summact; }
   values  { BREAK;   }
   values  { -NA-;    }
   values  { SUM;     }
   values  { COUNT;   }
   values  { MIN;     }
   values  { MAX;     }
   values  { AVG;     }
}


