// =================================================================
// Documentation System
//
// This is a general purpose hierarchy of documents.  It was
// developed first in the adocs app.  The tables are intended to be
// the cornerstone of all self-documenting features.
//
// 12/12/06 -Modified for cascade_delete, make everybody's life
//           easier, since all docs in this system are
//           generated out of code.
//
// 4/28/06 - Added to andro_universal.add to be used for generating
//           one page to describe each table.  Descriptions of code
//           and other stuff to follow.
// =================================================================

// This was from adocs, we are not ready for this yet
// menu add_media   { description: Add Image; module: operations; uisort: 0; }
column filepath { description: File Path;     type_id: vchar; colprec: 100; }
column filecnts { description: File Contents; type_id: mime-x;              }
column filename { description: File Name;     type_id: vchar; colprec: 60;  }
// column itemptr   { description: File;      type_id: mime-f; }

// This stuff gets its own module
module docs {
   description: Documentation;
   uisort: 900;
   nomenu: Y;
   // KFD 7/24/08, give group configadmin full rights
   group configadmin {
       permsel: Y; permins: Y; permupd: Y; permdel: Y; nomenu: N;
   }
}

// Library program to generate pages out of database and code
menu x_docview { description: View Docs; module: docs; uisort: 100; uix2: Y; }
menu x_docgen  { description: Gen Docs;  module: docs; uisort: 200; uix2: Y; }

column pagename { description: Page Name; type_id: vchar;  colprec: 60; }
column pagetext { description: Page Text; type_id: text; }

