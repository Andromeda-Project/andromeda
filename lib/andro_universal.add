   values { 1013; Username and password no good;   Y; }
   values { 1014; Attempt page access w/o login;   N; }
   values { 1020; PW Reset Request - PHP;          Y; }
   values { 1021; PW Reset Request - DB;           Y; }
   values { 1025; PW Verify Request - PHP;         Y; }
   values { 1026; PW Verify Request - DB;          Y; }
}


