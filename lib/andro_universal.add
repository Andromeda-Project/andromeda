   column { business; payer_email; receiver_email; payer_id; receiver_id; }
   column { last_name; first_name; }
   column { payment_status; payment_gross; mc_currency; mc_gross; }
   column { pending_reason; payment_type;    }
   column { item_name; item_number; }
   column { verify_sign; payer_status; test_ipn; tax; quantity; shipping; }
   column { notify_version; }
   column { ts_ins; uid_ins; }

   group $LOGIN { permins: Y; permsel: Y; }
}

