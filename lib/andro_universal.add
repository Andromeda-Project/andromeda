//  TABLE: Users
//
// The column user_id is in andro_universal
column member_password {
	description: N/A;
	type_id: vchar;
	colprec: 20;
}
column user_name { description: Full Name; type_id: char; colprec: 40; }
column user_disabled {
	description: Account Disabled; type_id: cbool;
	automation_id: DEFAULT;
	auto_formula: N;
}
column user_pwkey    { description: PW Key; type_id: char; colprec: 32; }
column user_pwexp    { description: PW Exp; type_id: dtime; }

table users {
	description: Application Users;
	module: system;
    uisort: 109;
    group usermaint { nomenu: N; permsel: Y; permins: Y; permupd: Y; permdel: Y;}

	column user_id { primary_key: Y; uisearch: Y; }
	column user_name { uisearch: Y; }
   column { email; }
   column user_disabled { uino: Y; }
   column { user_pwkey; user_pwexp; hostip; uino: Y; }

   column { member_password; automation_id: BLANK; description: Simple Password; }
   column md5 { prefix: member_; description: Encrypted Password; uino: Y; }

   // Effective group stuff
   column flag { suffix: _newgroup;
      automation_id: DEFAULT; auto_formula: N;
      uino: Y;
   }
   column group_id {
      suffix: _eff;
      description: Effective Group;
      uino: Y;
      automation_id: BLANK;
   }

	//group andro_own  { permins: Y; permsel: Y; }

   group $LOGIN { permsel: Y; }

	projection email     { column { user_name; }}
	projection emailaddr { column { email; }}

   index email { idx_unique: Y; column { email; }}
}

