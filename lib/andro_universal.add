column payment_date  {description: Transaction Date; type_id: dtime; }
column payment_status{description: Pmt Status; type_id: vchar; colprec: 20; }
column payment_gross {description: Pmt Gross; type_id: money; }
column mc_currency   {description: MC Currency; type_id: vchar; colprec: 20; }
column mc_gross      {description: MC Gross;    type_id: money; }
column pending_reason{description: Pending Reason; type_id: vchar; colprec: 20; }
column item_name     {description: Item Name; type_id: vchar; colprec: 40; }
column item_number   {description: Item Number; type_id:vchar; colprec: 20; }
column business      {description: Prim Email; type_id: vchar; colprec: 50; }
column payment_type  {description: Payment Type; type_id: vchar; colprec: 20; }
column verify_sign   {description: Verify; type_id: vchar; colprec: 56; }
column payer_status  {description: Payer Status; type_id: vchar; colprec: 10; }
column test_ipn      {description: Test Flag; type_id: vchar; colprec: 1; }
column tax           {description: Tax;      type_id: money; }
column shipping      {description: Shipping; type_id: money; }
column quantity      {description: Quantity; type_id: int; }
column payer_email   {description: Payer Email;   type_id: char; colprec: 50; }
column receiver_email{description: Receiver Email;type_id: char; colprec: 50; }
