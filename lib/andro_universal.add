// -- Pages themselves
table docpages {
   module: docs;
   description: Pages;
   nomenu: Y;

   column pagename    { uisearch: Y; primary_key: Y; }
   foreign_key docpages {
      suffix: _par;
      allow_empty: Y;
      uisearch:Y;
      delete_cascade: Y;
      column pagename_par { description: Parent Page; }
   }
   column sequence { description: Sequence; uisearch: Y; }

   column { pagetext; }

   //foreign_key docpageshier { auto_insert: Y; nocolumns: Y; }
   //column pagehier { uino: Y;
   //   chain calc {
   //      test { compare: @pagename_par NULL; return: 1; }
   //      test { compare: @pagename_par EMPTY; return: 1; }
   //      test { return: 0; }
   //   }
  // }
   column filename { automation_id: BLANK; }

   column { ts_ins; uid_ins; ts_upd; uid_upd; }

   group $LOGIN { permsel: Y; }
   //group doccommit { permupd: Y; permins: Y; }
}

// =========================================================
// The lowly scripts table, which tracks which scripts
// have been successfully run
// =========================================================
column script { type_id: vchar; colprec: 30; description: Script Name; }
table scripts {
    module: system;
    description: Completed Upgrade Scripts;
    nomenu: Y;

    column script { primary_key: Y; uisearch: Y; }
}


// =========================================================
// Import map tables.  Everybody is allowed full access,
// though eventually we want row-level security based on
// your connection to a table
// =========================================================
column importmap { type_id: char; colprec: 10; description: Map Name; }
table importmaps {
   module: system;
   description: Import Maps;
   nomenu: Y;
   group $LOGIN { permsel:Y;permins:Y;permupd:Y;permdel:Y; }

   column importmap { primary_key: Y; uisearch: Y; }
   column table_id  { primary_key: Y; uisearch: Y; }
   column name { suffix: _prefix; }
}
table importmapcolumns {
   module: system;
   description: Import Map Columns;
   nomenu: Y;
   group $LOGIN { permsel:Y;permins:Y;permupd:Y;permdel:Y; }

   foreign_key importmaps { primary_key: Y; }
   column column_id { description: Target Column; primary_key: Y; uisearch: Y; }
   column column_id { suffix: _src; description: Source;uisearch: Y;  }
}
