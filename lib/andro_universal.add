column paypal_v2 {
    type_id: cbool; description: Paypal returns to x_paypalfinal.php;
}
column auth_net_debugging {
    type_id: cbool; description: Authorize.net debugging;
}
column auth_net_testing {
    type_id: cbool; description: Authorize.net testing;
}
column auth_net_retries {
    type_id: numb; colprec: 1; description: Authorize.net Retries;
}
column auth_net_login_id {
    type_id: vchar; colprec: 15; description: Authorize.net Login ID;
}
column auth_net_tran_key {
    type_id: vchar; colprec: 15; description: Authorize.net Trnx Key;
}
table configfw {
    module: system;
    uisort: 550;
    description: Configuration - Framework;

    group $LOGIN { permsel: "Y"; nomenu: "Y"; }

    column recnum  { primary_key: "Y"; uisearch: "Y"; }
    column { loginbefore; loginafter; }
    column notes {
        suffix: footer;
        description: Page Footer Text;
    }
    column { smtpout; email_fromname; email_fromaddr; email_admin; }
    column { uisearchdash; }
    column { x4menu; x4padmenu; x4menutitle; x4menutext }
    column { js_css_debug; admin_logging; deprecated; sql_limit }
    column flag { suffix: _syslog; description: Log SQL to syslog }
    column { logout_clear; }
    column { paypal_site_url; paypal_image_url; paypal_payto;
        paypal_noship; paypal_v2;
        paypal_test_mode; paypal_fake;
    }
    column {
        auth_net_debugging; auth_net_testing; auth_net_retries;
        auth_net_login_id; auth_net_tran_key;
    }
}
table configapp {
    module: system;
    uisort: 551;
    description: Configuration - Application;

    group $LOGIN { permsel: "Y"; nomenu: "Y";  }

    column recnum  { primary_key: "Y"; uisearch: "Y"; }
    column { cf_template; }
    column { smtpout;     }
}
table configinst {
    module: system;
    uisort: 552;
    description: Configuration - Instance;

    group $LOGIN { permsel: "Y"; }
    group configadmin { permupd: "Y"; permsel: "Y"; nomenu: "N"; }

    column recnum  { primary_key: "Y"; uisearch: "Y"; }
    column { smtpout; }
}
table configuser {
    module: system;
    uisort: 553;
    description: My Configuration;

    group $LOGIN { permsel: Y; permins: Y; permupd: Y;  nomenu: N; }

    column uid_ins {
        primary_key: "Y";
        uisearch: "Y";
        group configadmin {
            permrow: "N";
        }
    }
}
menu configconfirm {
    module: system;
    uisort: 554;
    description: Configuration Review;
    group $LOGIN { permsel: Y; permins: Y; nomenu: N; }
}
content configfw {
    columns { smtpout; js_css_debug; uisearchdash;
        x4menu; x4padmenu; sql_limit; flag_syslog
    }
    update { localhost; N; --; N; N; 300; Y }
}



