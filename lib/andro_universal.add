//  TABLE: Cross-reference users to node groups
//
table usersxgroups {
	description: User - Group Assignments;
	module: system;
	nomenu: Y;
    group usermaint { permsel: Y;permins: Y;permupd: Y;permdel: Y;}

	foreign_key { table_id_par: users; primary_key: Y; uisearch: Y;
		uidisplay: mover;
	}
	foreign_key { table_id_par: groups; primary_key: Y; uisearch: Y;
		uidisplay: mover;
	}

   group $LOGIN { permsel: Y; }
}

// TABLE: Password change requests and request verification attempts
table users_pwrequests {
   description: Password Change Requests;
   nomenu: Y;
   module: usermaint;

   group $LOGIN { permins: Y; permupd:N;permsel:N;permdel:N; }

   column recnum  { suffix: _pwr; primary_key: Y; }
   column user_id { uisearch: Y; description: User ID;   }
   column md5     { uisearch: Y; description: Hash Code; }
   column { ts_ins; uid_ins; }
}

table users_pwverifies {
   description: Password Change Requests;
   nomenu: Y;
   module: system;

   group $LOGIN { permins: Y; permupd:N;permsel:N;permdel:N; }

   column recnum  { suffix: _pwv; primary_key: Y; }
   column user_id {               uisearch: Y; description: User ID;   }
   column md5     {               uisearch: Y; description: Hash Code; }
   column { member_password; automation_id: BLANK; description: New Password; }
   column { ts_ins; uid_ins; }
}



// =================================================================
// Adminstration table. Emails sent
// =================================================================
column adm_email     { description: Email No; type_id: int; }
column email_to      { description: TO;       type_id: char; colprec: 200; }
column email_subject { description: SUBJECT;  type_id: char; colprec: 200; }
column email_message { description: MESSAGE;  type_id: text; }
table adm_emails {
	description: Emails Sent by System;
	column_prefix: adm_email;
    uisort: 100;
	module: system;

	column adm_email { primary_key: Y; automation_id: SEQUENCE; }
	column email_to { uisearch: Y; }
	column email_subject { uisearch: Y; }
	column { email_message; }

	column { ts_ins;uid_ins }

	// Anybody can insert into this table, but nobody can
	// update or delete
	group $LOGIN { permins: Y; }
}


// =================================================================
// System Variables
// User-defined system variables control how things behave
// =================================================================
column variable       { description: Variable Name; type_id: char; colprec: 25; }
column variable_value { description: Value; type_id: text; }

table variables {
	description: System Variables;
    uisort: 900;
	module: system;

	column variable { primary_key: Y; uisearch: Y; }
	column description { uisearch: Y; }
	column variable_value { uisearch: Y; }

	group $LOGIN { permsel: Y; }
}

content variables {
   columns { variable; description; variable_value; }
   values  { SMTP_SERVER; Outgoing email server; localhost; }
}

