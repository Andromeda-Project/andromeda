column paypal_site_url {
    type_id: vchar; colprec: 200; description: Paypal callback URL;
}
column paypal_image_url {
    type_id: vchar; colprec: 200; description: Paypal Image (logo) URL;
}
column paypal_payto {
    type_id: vchar; colprec: 200; description: Paypal pay to;
}
