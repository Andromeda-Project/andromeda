// --------------------------------------------------------------------
// Group that can do user maintenance (and file maintenance)
// --------------------------------------------------------------------
// This is the super-power group.  Anybody in here has total control
// of the system.
group usermaint {
   permrole: Y;   // Can create/delete users
   description: User Administrator;

   // have full permissions on all user tables
   module usermaint {
      permsel: Y;
      permins: Y;
      permupd: Y;
      permdel: Y;
      nomenu:  N;
   }
}

// People here can
group filemaint {
   description: File Maintenance;
}
menu apppub {
    module: system;
    description: File Maintenance;
    uisort: 999;

    group filemaint {
        permsel: Y; nomenu: N;
    }
}

